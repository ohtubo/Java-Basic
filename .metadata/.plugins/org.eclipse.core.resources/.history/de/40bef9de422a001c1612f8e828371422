package practice;

public class Car {
	private int fuelCost;
	private double fuelAmount;
    // コンストラクタを作成
	public void instanceMethodPrint(int fuelCost, double fuelAmount) {
		System.out.println(fuelCost + "km走ります");
		System.out.println("残量は、" + fuelAmount + "です");
	}
    // moveメソッド
    // ・"xx km 走ります"を出力
    // ・残量を計算

    // fuelAmountを取得するメソッドを作成

    public double getFuelAmount() {
        return fuelAmount;
    }
	
}
